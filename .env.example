# Zcash RPC Configuration
# Copy this file to .env and fill in your values

# Zcash RPC endpoint URL
# Default for testnet: http://localhost:18232
# Default for mainnet: http://localhost:8232
ZCASH_RPC_URL=http://localhost:18232

# Zcash RPC authentication credentials
# These should match the rpcuser and rpcpassword in your zcash.conf file
ZCASH_RPC_USER=your_rpc_username
ZCASH_RPC_PASSWORD=your_rpc_password

# Viewing key for the z-address you want to track
# This is obtained by running: zcash-cli z_exportviewingkey <z-address>
# Example formats:
# - Testnet: zviewtestsapling...
# - Mainnet: zviews...
ZCASH_VIEWING_KEY=your_viewing_key_here

# Network selection (testnet or mainnet)
ZCASH_NETWORK=testnet
